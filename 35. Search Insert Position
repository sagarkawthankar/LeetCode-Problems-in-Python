class Solution:
    def searchInsert(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        if target<=nums[0]:
            return(0)
        for i in range(0,len(nums)):
            if nums[i]==target:
                return(i)
        smallerindex = 0
        for i in range(0,len(nums)):
            if nums[i]<=target:
                smallerindex = i
        return(smallerindex+1)
        
