class Solution:
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        if len(digits)==0:
            return(0)
        y = ''.join(map(str, digits))
        y = int(y)
        y = y+1
        y = str(y)
        digits = []
        for x in y:
            digits.append(int(x))
        return(digits)
